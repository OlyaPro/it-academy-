{
    "env": {
        "node": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 12
    },
    "extends": [
        "airbnb-base",
        "eslint:recommended",
        "plugin:import/errors",
        "plugin:import/warnings"
    ],
    "rules": {
        "no-async-promise-executor": "off",
        "import/no-unresolved": "error",
        "lines-between-class-members": "off",
        "import/prefer-default-export": "warn",
        "class-methods-use-this": "off",
        "import/no-cycle": "off",
        "camelcase": "warn",
        "no-useless-constructor": "warn",
        "guard-for-in": "off",
        "no-multi-spaces": [
            "error",
            {
                "ignoreEOLComments": true
            }
        ],
        "no-console": [
            "error",
            {
                "allow": [
                    "warn",
                    "error",
                    "info"
                ]
            }
        ],
        "no-shadow": "off",
        "no-empty-function": [
            "error",
            {
                "allow": [
                    "constructors"
                ]
            }
        ],
        "prefer-destructuring": [
            "error",
            {
                "object": false,
                "array": false
            }
        ],
        "no-undefined": "error",
        "no-unused-vars": "warn",
        "no-plusplus": "off",
        "linebreak-style": [
            "off"
        ],
        "arrow-parens": "off",
        "func-names": [
            "off"
        ],
        "no-param-reassign": [
            "off"
        ],
        "quotes": [
            "error",
            "single",
            {
                "allowTemplateLiterals": true,
                "avoidEscape": true
            }
        ],
        "prefer-arrow-callback": [
            "off"
        ],
        "dot-notation": [
            "error",
            {
                "allowPattern": "[a-zA-Z]"
            }
        ],
        "no-underscore-dangle": [
            "error",
            {
                "allow": [
                    "___initialData___",
                    "_request",
                    "_method",
                    "_baseSelector",
                    "_bodenPoint",
                    "_baseSelectorCity"
                ]
            }
        ]
    },
    "settings": {
        "import/parsers": {
            "eslint/parser": [
                ".js"
            ]
        },
        "parserOptions": {
            "sourceType": "module"
        },
        "plugins": [
            "import",
            "playwright"
        ]
    },
    "globals": {
        "playwright": "readonly"
    }
}